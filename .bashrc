# common commands
alias q='exit'
alias c='clear'
alias h='history'
alias cs='clear;ls'
alias p='cat'
alias lsa='ls -a'
alias lsl='ls -l'
alias lsal='ls -al'
alias o=open

# common directories
alias home='cd ~'
alias root='cd /'

# common project folders
alias src='cd ~/src'

# python env commands
alias pyenv='source env/bin/activate'

# ssh commands
alias caen='ssh caen'

# git
alias g='git'
alias st='git status'
alias com='git commit -m'
alias clone='git clone'
alias lg='git log'
alias all='git add .'

# apps and links
alias spotify='open -a spotify'
alias mail='open https://mail.google.com/'
alias canvas='open https://umich.instructure.com/'
alias netflix='open https://www.netflix.com/'

